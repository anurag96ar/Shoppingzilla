      font-weight: 600;
      text-align: center;
      text-transform: none;
      word-wrap: break-word;
    }

    .swal2-styled.swal2-confirm {
      border: 0;
      border-radius: 0.25em;
      background: initial;
      background-color: #4fbfa8;
      color: #fff;
      font-size: 14px;
    }

    .swal2-styled {
